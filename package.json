{
  "name": "BigTrip",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Большое путешествие» от HTML Academy",
  "scripts": {
    "eslint": "eslint src/**/*.js",
    "test": "npm run eslint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "deploy": "NODE_ENV=production webpack",
    "publish": "git checkout gh-pages && 
               git merge master &&
               npm run deploy &&
               git commit -am 'Deploy' &&
               git push origin gh-pages &&
               git checkout @{-1}"
},
  },
  "devDependencies": {
    "css-loader": "3.4.1",
    "eslint": "6.1.0",
    "eslint-config-htmlacademy": "0.5.2",
    "moment-locales-webpack-plugin": "1.1.2",
    "style-loader": "1.1.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "chart.js": "2.9.3",
    "chartjs-plugin-datalabels": "0.7.0",
    "flatpickr": "4.6.3",
    "gh-pages": "^3.0.0",
    "gh-pages-webpack-plugin": "^1.0.2",
    "moment": "2.24.0",
    "nanoid": "2.1.11"
  }
}
